.none{
	width:100%;
	height:100%;
	text-align:center;
	vertical-align:middle;
	overflow:hidden;
}
.none img{
	width:60%;
	margin-top:40%;
}
@media screen and (orientation:landscape){
	.none img{
		width:60%;
		margin-top:15%;
	}
}
@media screen and (orientation:portrait){
	.none img{
		width:60%;
		margin-top:40%;
	}
}