header{
	background:#04be02;
	color:#fff;
	padding:3%;
	line-height:2em;
	width:95%;
	position:fixed;
	left:0;
	top:0;
	z-index:10;
}
header .return{
	float:left;
	width:25%;
}
header .return span{
	width:5%;
	color:#fff;
}
header .return a{
	width:95%;
	color:#fff;
}
header .title{
	float:left;
	text-align:center;
	font-size:1.5em;
	width:50%;
}
header .label{
	float:right;
	width:25%;
	text-align:right;
}
header .label a{
	display:block;
	width:100%;
	color:#fff;
}