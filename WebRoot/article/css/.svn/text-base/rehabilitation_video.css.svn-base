body{max-width:640px;margin:0 auto;background-color:#fff;}
/*************rehabilitation_video************/
.v-nav{position:fixed;left:0;top:0;z-index:10;width:100%;height:50px;background-color:#f2f2f2;display:flex;display: -webkit-flex;}
.v-nav li{flex-wrap: nowrap;flex:1;flex-grow:1;box-sizing:border-box;line-height:50px;text-align:center;}
.v-nav li span{padding:2px 8px;border:1px solid #f2f2f2;border-radius:16px;color:#f49162;}
.v-nav li span.vb{color:#c07ecf;}
.v-nav li span.vc{color:#98cc37;}
.v-nav li.active{background-color:#e0e0e0;}
.v-nav li.active span{border:1px solid #f49162;background-color:#fff;}
.v-nav li.active span.vb{border:1px solid #c07ecf;}
.v-nav li.active span.vc{border:1px solid #98cc37;}

.v-title{width:100%;height:40px;margin-top:60px;text-indent:26px;line-height:40px;font-weight:normal;font-size:15px;border-bottom:1px solid #eee;background:url(../images/icon_circle.png) no-repeat 6px center;background-size:20px;}
.v-list{width:100%;}
.v-list dl{float:left;width:47%;height:165px;margin-top:15px;border-bottom:1px solid #f1f1f1;}
.v-list dl:nth-of-type(odd){padding-left:2%;padding-right:1%;}
.v-list dl:nth-of-type(even){padding-left:1%;padding-right:2%;}
.v-list dl img{display:block;width:100%;height:96px;}
.v-list dl dt{width:100%;height:36px;margin-top:6px;line-height:18px;font-size:15px;color:#000;
	display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    word-break: break-all;}
.v-list dl dd{font-size:12px;color:#888;}
.v-list dl dd span{margin-right:10px;}
